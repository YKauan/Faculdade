algoritmo "Qst08Vetor"\par
  var\par
      name       :vetor [1..8] de caractere\par
      valorVenda :vetor [1..8] de real\par
      valorcusto:vetor[1..8] de real\par
      temp:real\par
      i,i2:inteiro\par
  inicio\par
\par
     para i de 1 ate 8 faca\par
     escreval()\par
     escreva("Insira o nome do: ",i,"\'ba produto: ")\par
     leia(name[i])\par
     escreva("Valor de Venda do(a): ",name[i],": ")\par
     leia(valorVenda[i])\par
     escreva("Valor de Custo do(a): ",name[i],": ")\par
     leia(valorCusto[i])\par
\par
   fimPara\par
\par
\par
   para i de 1 ate 8-1 faca\par
      para i2 de i+1 ate 8 faca\par
         se valorcusto[i] > valorcusto[i2] entao\par
            temp<- valorcusto[i]\par
            valorcusto[i]<- valorcusto[i2]\par
            valorcusto[i2]<- temp\par
         fimse\par
      fimpara\par
   fimpara\par
   \par
   escreval()\par
   escreval("=====5 Produtos com o maior custo=====")\par
       para i de 8 ate 1 passo -1 faca\par
              escreval(valorcusto[i])\par
       fimpara\par
       i<i+1\par
       enquanto  valorcusto[1]  <> valorcusto[5] faca\par
                 escreva("---",valorcusto[i])\par
       fimenquanto\par
\par
fimalgoritmo\par
