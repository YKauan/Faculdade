algoritmo "Qst07Vetor"\par
      var\par
   n:vetor[1..8] de inteiro\par
   i,i2,temp:inteiro\par
inicio\par
\par
    n[1] <- 5\par
    n[2] <- 4\par
    n[3] <- 7\par
    n[4] <- 3\par
    n[5] <- 2\par
    n[6] <- 6\par
    n[7] <- 1\par
    n[8] <- 8\par
\par
   para i de 1 ate 8-1 faca\par
      para i2 de i+1 ate 8 faca\par
         se n[i] > n[i2] entao\par
            temp<- n[i]\par
            n[i]<- n[i2]\par
            n[i2]<- temp\par
         fimse\par
      fimpara\par
   fimpara\par
\par
   escreval("=====ORDEM CRESCENTE=======")\par
   para i de 1 ate 8 faca\par
      escreval(n[i])\par
   fimpara\par
   escreval("=====ORDEM DECRESCENTE=====")\par
       para i de 8 ate 1 passo -1 faca\par
              escreval(n[i])\par
       fimpara\par
\par
fimalgoritmo\par
