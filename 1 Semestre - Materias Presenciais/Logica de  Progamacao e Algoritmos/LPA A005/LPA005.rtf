algoritmo "Qst05Vetor"\par
    var\par
        tt : vetor [1..12] de real\par
        vlr,vlrjur : real\par
        i,o,qtdm : inteiro\par
\par
\par
inicio\par
\par
    o <- 0\par
    escreval("Taxa fixa de 2.5%")\par
    escreva("Informe o valor: ")\par
    leia(vlr)\par
    escreva("Informe o periodo: ")\par
    leia(qtdm)\par
\par
\par
    enquanto i < qtdm faca\par
      vlrjur <- vlr * 0.025\par
      vlr <- vlr + vlrjur\par
      i <- i + 1\par
      tt[i]<- vlr\par
    fimenquanto\par
        \par
   para i de 1 ate 12 faca\par
   \tab\tab escreval("Valor: ", I, "\'ba mes: ",tt[i])\par
\tab fimpara\par
\par
fimalgoritmo\par
