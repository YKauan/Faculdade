algoritmo "Qst04Vetor"\par
    var psg  : vetor [1..3] de caractere\par
        nv  : vetor [1..3] de inteiro\par
        p  : vetor [1..3] de inteiro\par
        psgs: caractere\par
        i,nvs,ps,d: inteiro\par
    inicio\par
\par
    escreva("Digite a quantidade de passageiros:")\par
    leia(d)\par
    para i de 1 ate d faca\par
             escreva("Digite o nome do passageiro: ")\par
             leia(psgs)\par
             escreva("Digite o numero do voo: ")\par
             leia(nvs)\par
             escreva("Digite o numero da poltrona: ")\par
             leia(ps)\par
             psg[i]<- psgs\par
             nv[i]<- nvs\par
             p[i]<- ps\par
    fimpara\par
       para i de 1 ate d faca\par
         escreval(psg[i],":")\par
   \tab\tab escreval("Numero do voo: ",nv[i]," - Poltrona: ",p[i])\par
\tab fimpara\par
\par
    fimalgoritmo\par
