algoritmo "Qst02Vetor"\par
    var n  : vetor [1..3] de caractere\par
        p  : vetor [1..3] de real\par
        a  : vetor [1..3] de real\par
        id : vetor [1..3] de inteiro\par
        peso,altura: REAL\par
        name: caractere\par
        i,idd: inteiro\par
    inicio\par
\par
    para i de 1 ate 3 faca\par
             escreva("Digite o nome do individuo: ")\par
             leia(name)\par
             escreva("Digite sua identidade: ")\par
             leia(idd)\par
             escreva("Digite seu peso: ")\par
             leia(peso)\par
             escreva("Digite sua altura: ")\par
             leia(altura)\par
             n[i]<- name\par
             p[i]<- peso\par
             a[i]<- altura\par
             id[i]<- idd\par
        fimpara\par
        para i de 1 ate 3 faca\par
         escreval(n[i],":")\par
   \tab\tab escreval("Identidade: ",id[i],"- Altura: ",a[i],"- Peso: ",p[i])\par
\tab fimpara\par
\par
    fimalgoritmo\par
