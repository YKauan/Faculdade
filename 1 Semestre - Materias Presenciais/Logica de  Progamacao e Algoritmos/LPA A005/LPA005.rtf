\pard\tx720\cf1\f0\fs38 =============QST01===========================\par
algoritmo "Qst01Vetor"\par
    var nu1: vetor [1..6] de real\par
        nu2: vetor [1..6] de real\par
        MD: vetor [1..6] de real\par
        NP1,NP2,S: REAL\par
        ALU: vetor [1..6] de caractere\par
        alun: caractere\par
        i: inteiro\par
    inicio\par
    \par
    para i de 1 ate 6 faca\par
             escreva("Digite o nome do aluno: ")\par
             leia(alun)\par
             escreva("Digite sua nota referente a NP1: ")\par
             leia(NP1)\par
             escreva("Digite sua nota referente a NP2: ")\par
             leia(NP2)\par
             ALU[i]<- alun\par
             nu1[i]<-NP1\par
             nu2[i]<-NP2\par
             S <- NP1 + NP2\par
             S <- S / 2\par
             MD[i]<- S\par
        fimpara\par
        para i de 1 ate 6 faca\par
         escreval(ALU[i],":")\par
   \tab\tab escreval("Sua nota N1 e: ", nu1[i], " Sua nota N2 e: ",nu2[i]," Sua media e: ",MD[i])\par
\tab fimpara\par
    \par
    fimalgoritmo\par
