          escreva("Informe o nome do aluno: ")\par
          leia(alun)\par
          name[i]<-alun\par
          escreva("Confirmar presenca: S para presente / N para falta: ")\par
          leia(F)\par
          fts[i]<-F\par
    fimpara\par
\par
        para i de 1 ate 20 faca\par
   \tab\tab escreval("Aluno: ",name[i]," - Presente: ",fts[i])\par
\tab fimpara\par
\par
fimalgoritmo\par
